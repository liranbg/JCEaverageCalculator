		confirmQuestion  = confirmQuestion.replace(&quot;\\n&quot;,&quot;\n&quot;);
		confirmQuestion  = confirmQuestion.replace(&quot;\\n&quot;,&quot;\n&quot;);
		result = confirm (confirmQuestion);
		if (!result)
