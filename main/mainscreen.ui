function SubmitForm(Obj,app, prg, arg, trg)
{
	if(Obj==null)
		Obj=event.srcElement;
	if(Obj==null)
		return;

	bConfirm=Obj.getAttribute(&quot;Confirm&quot;);
	if (bConfirm==&quot;true&quot;)
